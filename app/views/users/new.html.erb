<h1> Sign Up </h1>

<%= form_for(:user, url: {:action => 'create', :controller => 'users'}) do |f| %>
  <p>First Name: <%= f.text_field :first_name %> </p>
  <p>Last Name: <%= f.text_field :last_name %> </p>
  <p>Email: <%= f.text_field :email %> </p>
  <p>Password <%= f.password_field :password %> </p>

  <%= f.submit "Sign Up" %>
<% end %>
