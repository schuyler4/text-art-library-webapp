<div class="container">
  <h1> Create A Title </h1>

  <% if @animated_art.errors.any? %>
    <% @animated_art.errors.full_messages.each do |msg| %>
          <h3><%= msg %></h3>
    <% end %>
  <% end %>

  <%= form_for :animated_art, url: {:action=>"create", :controller=>"animated_art"} do |f| %>
      <%= f.text_field :title, class: "form-control", placeholder: "Title" %>
      <%= f.submit %>
  <% end %>
</div>
